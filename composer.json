{
    "name": "emmajiugo/rave-cli",
    "description": "A CLI app that helps you test Rave payment on the go and also generate sample apps.",
    "keywords": ["framework", "flutterwave rave", "rave", "rave cli", "rave console", "cli"],
    "homepage": "https://flutterwave.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/emmajiugo/rave-cli-php/issues",
        "source": "https://github.com/emmajiugo/rave-cli-php"
    },
    "authors": [
        {
            "name": "Chigbo Ezejiugo",
            "email": "emmajiugo@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel-zero/framework": "5.7.*",
        "vlucas/phpdotenv": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "ext-posix": "0"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php application app:rename"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["rave"]
}
